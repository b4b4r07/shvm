#!/usr/bin/env bash

help() {
  echo -n "\
Usage: bashvm use [--default] <version>

  --default   set the default bash version to <version>
  --install   install the version if it is not in the local
"
}

while test $# != 0; do
  case "$1" in
  --help)
    help
    exit 0
    ;;
  esac
  shift
done
